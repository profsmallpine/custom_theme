<p id="notice"><%= notice %></p>

<h1 id="theme-header">Listing Themes <%= Theme.return_whitelabel %></h1>

<table>
  <thead>
    <tr>
      <th>Tenant</th>
      <th>Font color</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @themes.each do |theme| %>
      <tr>
        <td><%= theme.tenant %></td>
        <td><%= theme.font_color %></td>
        <td><%= link_to 'Show', theme %></td>
        <td><%= link_to 'Edit', edit_theme_path(theme) %></td>
        <td><%= link_to 'Destroy', theme, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Theme', new_theme_path %>

<br/><br/>
